{
	"uid"		:	"node-red-runner",
	"append"	: 	true,
	"watch"		: 	false,
	"script"	: 	"node-red",
	"args"		: 	["-s", "/home/{USER}/node-red-user-dir/settings.js", "-u", "/home/{USER}/node-red-user-dir/", "-v"],
	"command"	: 	"sudo-node",
	"sourceDir"	: 	"/home/{USER}/.nvm/versions/io.js/v3.0.0/bin",
	"logFile"	: 	"/home/{USER}/node-red-user-dir/Node-red-forever.log",
	"outFile"	: 	"/home/{USER}/node-red-user-dir/logs/Node-red-out.log",
	"errFile"	: 	"/home/{USER}/node-red-user-dir/Node-red-err.log"
}

